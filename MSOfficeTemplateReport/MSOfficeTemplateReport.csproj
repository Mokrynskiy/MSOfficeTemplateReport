<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>MSOfficeTemplateReport</Title>
    <Authors>Aleksandr Mokrinskiy</Authors>
    <Company>Vector-Best</Company>
    <Product>MSOfficeTemplateReport</Product>
    <Description>Создание отчетов по шаблонам документов MSOffice</Description>
    <IncludeSymbols>True</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="3.0.2" />
    <PackageReference Include="OpenXmlPowerTools.NetStandard" Version="4.6.0" />
  </ItemGroup>

</Project>
